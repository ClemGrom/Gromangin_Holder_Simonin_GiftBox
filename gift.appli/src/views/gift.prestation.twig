{% extends "gift.main.twig" %}

{% block title %}Prestation {{ prestation.id }}{% endblock %}

{% block content %}
    <h1>Prestation {{ prestation.id }}</h1>
    <h1>Prestation {{ prestation.libelle }}</h1>
    <p>{{ prestation.description }}</p>
    <p>{{ prestation.tarif }}</p>
    <p>{{ prestation.unite }}</p>
    <a href="{{ url_for('prestations') }}">Liste de prestations</a>
{% endblock %}